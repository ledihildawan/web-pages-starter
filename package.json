{
  "name": "web-pages-starter",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "rsbuild dev",
    "build": "rsbuild build && bun run ./scripts/generate-sitemap.ts",
    "build:debug": "cross-env MINIFY=false rsbuild build && bun run ./scripts/generate-sitemap.ts",
    "build:no-html-minify": "cross-env MINIFY_HTML=false rsbuild build && bun run ./scripts/generate-sitemap.ts",
    "preview": "rsbuild preview",
    "gen": "bun run ./scripts/generate.ts",
    "clean": "rm -rf dist bun.lockb node_modules && bun install"
  },
  "dependencies": {
    "@splidejs/splide": "^4.1.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@rsbuild/core": "^1.7.1",
    "@rsbuild/plugin-image-compress": "^1.3.2",
    "@rstest/core": "^0.7.8",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/node": "^25.2.0",
    "bun-types": "^1.3.8",
    "cross-env": "^10.1.0",
    "glob": "^13.0.0",
    "happy-dom": "^20.0.11",
    "html-minifier-terser": "^7.2.0",
    "nunjucks": "^3.2.4",
    "postcss": "^8.5.6",
    "simple-nunjucks-loader": "^3.2.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
}
